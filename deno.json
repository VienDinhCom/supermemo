{
  "name": "@vien/supermemo",
  "version": "2.0.21",
  "license": "MIT",
  "exports": "./src/main.ts",
  "tasks": {
    "test": "deno test src",
    "release": "deno run -A npm:release-it",
    "build": {
      "command": "deno run -A build.ts",
      "dependencies": ["test"]
    },
    "publish": {
      "command": "deno publish --no-provenance && cd npm && npm publish",
      "dependencies": ["build"]
    }
  }
}
